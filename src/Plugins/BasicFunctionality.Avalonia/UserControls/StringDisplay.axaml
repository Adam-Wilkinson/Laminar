<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:uid="using:Laminar.PluginFramework.UserInterface.UserInterfaceDefinitions"
             mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="40" d:DataContext="{x:Static uid:StringViewer.DesignInstance}"
             x:DataType="uid:StringViewer+UITarget"
             x:Class="BasicFunctionality.Avalonia.UserControls.StringDisplay">
	<DockPanel>
		<TextBlock DockPanel.Dock="Right" Name="ValueViewer"/>
		<TextBlock DockPanel.Dock="Left" Text="{Binding Name}"/>
	</DockPanel>
</UserControl>
